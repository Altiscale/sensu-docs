{
    "root": "/app/public/",
    "https_only": true,
    "error_page": "/404.html",
    "routes": {
        "/sensu-core/latest/?(.*)": "/sensu-core/1.8/$1",
        "/uchiwa/latest/?(.*)": "/uchiwa/1.0/$1",
        "/sensu-enterprise/latest/?(.*)": "/sensu-enterprise/3.6/$1",
        "/sensu-enterprise-dashboard/latest/?(.*)": "/sensu-enterprise-dashboard/2.16/$1",
        "/sensu-go/latest/?(.*)": "/sensu-go/5.14/$1",
        "/plugins/latest/?(.*)": "/plugins/1.0/$1"
    },
    "rewrites": {
      "~ ^/sensu-core/2.0/?(.*)$": {
          "url": "/sensu-go/5.0/$1",
          "status": 301
        },
        "~ ^/sensu-go/latest/guides/create-a-ready-only-user$": {
            "url": "/sensu-go/latest/guides/create-read-only-user",
            "status": 301
        }
    }
}
